###########
# GIT stuff
###########
source ~/.git_prompt
export PS1='\n$? [\W] \e[0;32m$(__git_ps1 " (%s)")\e[0m\n$ '
export CLICOLOR=1
export LSCOLORS=GxFxCxDxBxegedabagaced

[[ -f ~/.git-completion.bash ]] && . ~/.git-completion.bash

alias git=hub

############
# Own sripts
############
export PATH=$PATH:$HOME/projects/scripts

#######
#Nodejs
#######
export NVM_DIR="/Users/amazesoft/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && . "$NVM_DIR/nvm.sh"  # This loads nvm

#######
#RVM
#######
export PATH="$PATH:$HOME/.rvm/bin" # Add RVM to PATH for scripting
[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm" # Load RVM into a shell session *as a function*

export JAVA_HOME=`/usr/libexec/java_home -v 1.8`

export WORKSPACE=$HOME/projects/BGCH

export AWS_ACCESS_KEY_ID=XXX
export AWS_SECRET_ACCESS_KEY=XXX

# eval `boot2docker shellinit 2>/dev/null`
# need 'default' machine to be running in virtual box
# create ~/Library/LaunchAgents/com.docker.machine.default.plist
eval $(docker-machine env default)

